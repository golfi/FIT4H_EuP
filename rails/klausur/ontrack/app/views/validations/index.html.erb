<p id="notice"><%= notice %></p>

<h1>Listing Validations</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>Comment</th>
      <th>Username</th>
      <th>Ranking</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @validations.each do |validation| %>
      <tr>
        <td><%= validation.project %></td>
        <td><%= validation.comment %></td>
        <td><%= validation.username %></td>
        <td><%= validation.ranking %></td>
        <td><%#= link_to 'Show', validation %></td>
        <td><%#= link_to 'Edit', edit_validation_path(validation) %></td>
        <td><%#= link_to 'Destroy', validation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Validation', new_project_validation_path %>
